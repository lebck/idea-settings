<application>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="14" />
    <option name="FONT_FAMILY" value="Roboto Mono" />
  </component>
  <component name="EditorSettings">
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" pattern="\b(doit)\b.*" />
    <filter name="MyToDos">
      <pattern index="1" />
    </filter>
  </component>
</application>