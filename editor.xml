<application>
  <component name="DefaultFont">
    <option name="FONT_FAMILY" value="Roboto Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Regular" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
  </component>
  <component name="EditorSettings">
    <option name="STRIP_TRAILING_SPACES" value="Whole" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="InlayHintsSettings">
    <option name="lastViewedProviderKeyId" value="parameter.hints.old" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern icon="important" useCustomColors="true" pattern="todo!.*">
      <option name="FOREGROUND" value="a19d00" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="2aa198" />
    </pattern>
    <filter name="Important">
      <pattern index="1" />
    </filter>
  </component>
</application>