    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="useCallback" value="const $NAME$ = useCallback(() =&gt; {$END$}, [])" description="" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="lessm" value="import css from './$FILENAME$.module.less';&#10;" description="CSS Module import" toReformat="false" toShortenFQNames="true">
